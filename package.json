{
  "name": "amavm-vdh-api",
  "version": "0.1.0",
  "description": "AMAVM VDH API",
  "private": true,
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "generate-schemas": "uno generate-schemas \"src/entities/*.ts\" -c ./tsconfig.json -f ts -o src/entities/schemas.ts",
    "generate-openapi-schemas": "uno generate-schemas \"src/entities/*.ts\" -c ./tsconfig.json -f openapi3 -o openapi.yml",
    "prestart": "npm run generate-schemas",
    "start": "node --inspect ./node_modules/serverless/bin/serverless offline start -s local",
    "test": "mocha --opts test/mocha.opts test/unit/**/*.test.ts",
    "test:watch": "mocha --opts test/mocha.opts --watch test/unit/**/*.test.ts",
    "test:e2e": "newman run test/e2e/e2e.collection.json",
    "cover": "nyc mocha --reporter mocha-junit-reporter test/unit/**/*.test.ts",
    "predeploy": "npm run generate-schemas",
    "deploy": "sls deploy -s",
    "prepkg": "npm run generate-schemas",
    "pkg": "sls package -s",
    "configure": "ts-node build/configure.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "cobertura",
      "html"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.339.0",
    "firebase-admin": "^8.9.2",
    "mime-types": "^2.1.21",
    "mongodb": "^3.1.8",
    "proj4": "^2.5.0",
    "source-map-support": "^0.5.9",
    "uno-serverless": "^0.51.0",
    "uno-serverless-aws": "^0.51.0",
    "uno-serverless-jwt": "^0.51.0",
    "uuid": "^3.3.2",
    "windows-1252": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/faker": "^4.1.4",
    "@types/geojson": "^7946.0.4",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.12",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.0",
    "@types/proj4": "^2.3.4",
    "@types/sinon": "^5.0.5",
    "@types/uuid": "^3.4.4",
    "cache-loader": "^1.2.2",
    "chai": "^4.2.0",
    "cryptiles": ">=4.1.2",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "newman": "^4.1.0",
    "nock": "^10.0.1",
    "node-ask": "^1.0.1",
    "nyc": "^13.1.0",
    "serverless": "^1.36.1",
    "serverless-api-stage": "^1.4.0",
    "serverless-aws-cn": "^0.1.0",
    "serverless-offline": "^4.0.0",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-webpack": "^5.2.0",
    "sinon": "^7.0.0",
    "thread-loader": "^1.2.0",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "tslint-sonarts": "^1.8.0",
    "typescript": "^3.1.3",
    "uno-serverless-cli": "^0.51.0",
    "webpack": "^4.22.0"
  }
}
